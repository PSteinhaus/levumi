<%if @newStudent.nil? ||@newStudent.blank?%>
<%else%>
  var studentsCrypt = JSON.parse(sessionStorage['students']);
  studentsCrypt["<%=@group.id%>"]["<%=@newStudent.id%>"] = newStudent;
  sessionStorage['students'] = JSON.stringify(studentsCrypt);
<%end%>
$('#students').html("<%= escape_javascript(render 'index')%>");
$('#studentForm').html("<%= escape_javascript(render 'full_form') %>");